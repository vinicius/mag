package core.brakes.de.fub.bytecode.generic;

/** 
 * ALOAD - Load reference from local variable
 * Stack ... -> ..., objectref
 *
 * @version $Id: ALOAD.java,v 1.2 2006-03-11 03:19:48 rafaelf Exp $
 * @author  <A HREF="http://www.inf.fu-berlin.de/~dahm">M. Dahm</A>
 */
public class ALOAD extends LocalVariableInstruction implements PushInstruction {
	private static final long serialVersionUID = 1866051040109338119L;
	/**
	 * Empty constructor needed for the Class.newInstance() statement in
	 * Instruction.readInstruction(). Not to be used otherwise.
	 */
	ALOAD() {
		super(ALOAD, ALOAD_0);
	}  
	public ALOAD(int n) {
		super(ALOAD, ALOAD_0, n);
	}  
}
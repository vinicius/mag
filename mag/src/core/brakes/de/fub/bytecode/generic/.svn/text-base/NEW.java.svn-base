package core.brakes.de.fub.bytecode.generic;


/** 
 * NEW - Create new object
 * <PRE>Stack: ... -&gt; ..., objectref</PRE>
 *
 * @version $Id: NEW.java,v 1.2 2006-03-11 03:19:48 rafaelf Exp $
 * @author  <A HREF="http://www.inf.fu-berlin.de/~dahm">M. Dahm</A>
 */
public class NEW extends CPInstruction implements LoadClass, AllocationInstruction {
  /**
	 * 
	 */
	private static final long serialVersionUID = 1340029296532663971L;
	
/**
   * Empty constructor needed for the Class.newInstance() statement in
   * Instruction.readInstruction(). Not to be used otherwise.
   */
  NEW() {}  
  public NEW(int index) {
	super(NEW, index);
  }  
}
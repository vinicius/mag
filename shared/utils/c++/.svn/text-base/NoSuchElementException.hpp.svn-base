#ifndef NoSuchElementException_HPP
#define NoSuchElementException_HPP

#include <string>

using std::string;

  class NoSuchElementException{

    private:

      string what_;

    public:

      NoSuchElementException(string what):what_(what){}

      const string & what() const{return what_; }

  };

#endif//NoSuchElementException_HPP
